[REFERENTTEST:#3@m 4].

[REFERENTTEST:Test 13*v4@M5].

[REFERENTTEST:test14#4@m6].

[REFERENTTEST: {}].

[REFERENTTEST: {*}].

[REFERENTTEST: {test17,*,#,*v1,#1,@m1,n1*v2,n2#2,n3@m1,n4*v3@m2,n5#3@m3}].

[REFERENTTEST: DISJ{test18,*v1,#1}].

[REFERENTTEST: COLL{test19,n1@m1}].

[REFERENTTEST: RESP{test20,n2#2}].

[REFERENTTEST:@A   B  C   ].

[REFERENTTEST:@2.54 CM].

[REFERENTTEST:@March 12 1987].

[REFERENTTEST: {@55 skidoo,@March 13 1987}].

